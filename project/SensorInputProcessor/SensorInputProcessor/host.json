{
  "version": "2.0",
  "extensions": {
    "eventHubs": {
      "transportType": "amqpWebSockets"
    }
  },
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "enableLiveMetricsFilters": true
    }
  }
}